<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Suite">


	<test name="AssertionTesting" enabled="true">
		<classes>
			<class name="assertMethodImplementation.Assertion">
				<methods>
					<include name="testHardAssert"></include>
					<include name="testSoftAssert"></include>
					<include name="title"></include>
				</methods>
			</class>
		</classes>
	</test>


	<test name="PDFRederTesting" enabled="true">
		<classes>
			<class name="com.pdf.reader.PDFReader">
				<methods>
					<include name="ReadPDFFile"></include>
				</methods>
			</class>
		</classes>
	</test>


	<test name="DataProviderTesting" enabled="true">
		<classes>
			<class name="DataProvider.DataProviderInteger">
				<methods>
					<include name="Testlogin"></include>

				</methods>
			</class>
		</classes>
	</test>

	<test name="DataProviderTesting2" enabled="true">
		<classes>
			<class name="DataProvider.TestDataProviders">
				<methods>
					<include name="goToFacebook"></include>

				</methods>
			</class>
		</classes>
	</test>


	<test name="ExamplesTesting" enabled="true">
		<classes>
			<class name="example.com.Alert">
				<methods>
					<include name="AlertBox"></include>

				</methods>
			</class>
		</classes>
	</test>

	<test name="ExamplesTesting2" enabled="true">
		<classes>
			<class name="example.com.Dropdown">
				<methods>
					<include name="main"></include>

				</methods>
			</class>
		</classes>
	</test>


</suite>
